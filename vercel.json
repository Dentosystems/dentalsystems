{
  "version": 2,
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/services",
      "dest": "/services.html"
    },
    {
      "src": "/contact",
      "dest": "/contact.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}